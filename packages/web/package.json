{
  "name": "@package/web",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "description": "front-end for application",
  "author": "Elias Mawa <elias@emawa.io>",
  "private": true,
  "homepage": "https://github.com/celestialstag/inkbyte#readme",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/celestialstag/inkbyte.git",
    "directory": "packages/utilities"
  },
  "scripts": {
    "lint": "echo 'linting @package/web' && eslint .",
    "prettier": "echo 'formatting @package/web' && yarn prettier  --plugin-search-dir . --write . --check .",
    "test": "echo 'test @package/web' && yarn test:unit && yarn test:web",
    "test:unit": "echo 'test:unit @package/web' && vitest",
    "test:web": "echo 'test:web @package/web' && playwright test",
    "watch": "vite --port $API_PORT",
    "serve": "echo 'serving @package/web' && cross-env ENV=development yarn watch",
    "optimize": "echo 'optimize @package/web:client' && cross-env ENV=production NODE_ENV=production vite optimize",
    "build": "echo 'build @package/web:client' && cross-env ENV=production NODE_ENV=production vite build",
    "preview": "echo 'preview @package/web' && cross-env ENV=production NODE_ENV=production yarn watch preview",
    "start": "echo 'starting @package/web' && cross-env ENV=production NODE_ENV=production yarn watch preview"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "~1.2.0",
    "@emotion/react": "~11.10.5",
    "@emotion/server": "~11.10.0",
    "@emotion/styled": "~11.10.5",
    "@lib/components": "~0.0.1",
    "@lib/config": "~0.0.1",
    "@lib/data": "~0.0.1",
    "@lib/hook-form": "~0.0.1",
    "@lib/hooks": "~0.0.1",
    "@lib/quikk": "~0.0.1",
    "@lib/schema-validator": "~0.0.1",
    "@lib/shared": "~0.0.1",
    "@lib/stores": "~0.0.1",
    "@mantine/carousel": "~6.0.10",
    "@mantine/core": "~6.0.10",
    "@mantine/form": "~6.0.10",
    "@mantine/hooks": "~6.0.10",
    "@mantine/ssr": "~6.0.10",
    "@playwright/test": "~1.28.1",
    "appwrite": "~11.0.0",
    "compression": "~1.7.4",
    "date-fns": "~2.30.0",
    "evergreen-ui": "~7.1.5",
    "express": "~4.18.2",
    "immer": "~9.0.21",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-icons": "~4.8.0",
    "sirv": "~2.0.2",
    "valtio": "~1.10.5",
    "yup": "~1.2.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "~11.10.5",
    "@types/compression": "~1.7.2",
    "@types/express": "~4.17.14",
    "@types/node": "~18.11.9",
    "@types/react": "~18.0.10",
    "@types/react-dom": "~18.0.10",
    "@vitejs/plugin-react": "~2.0.1",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "~4.6.0",
    "sass": "~1.56.2",
    "ts-node": "~10.9.1",
    "typescript": "~5.0.2",
    "vite": "~4.2.1",
    "vite-plugin-pwa": "~0.14.7",
    "vite-plugin-ssr": "~0.4.123"
  }
}
