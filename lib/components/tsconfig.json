{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist",
    "target": "es2020",
    "module": "es6",
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"]
    // "noEmit": false,
    // "declaration": true,
    // "declarationMap": true,
    // "skipLibCheck": true
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./typings/**/*.d.ts",
    "../../typings/**/*.d.ts",
    "typings/types"
  ],
  "exclude": ["./node_modules/", "./dist/**/*"]
}
